{
  "storage": "../.data/local-registry",
  "auth": { "htpasswd": { "file": "./htpasswd" } },
  "uplinks": {
    "npmjs": {
      "url": "https://registry.npmjs.org/",
      "cache": true
    }
  },
  "packages": {
    "**": {
      "access": "$all",
      "publish": "$all",
      "unpublish": "$all",
      "proxy": "npmjs"
    }
  }
}
